        <item  name="android:windowBackground">@drawable/finallogo</item>

    </style>

    <style
        name="MyTheme"
        parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->

        <item name="colorPrimary">@color/toolbar_color</item>
